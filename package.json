{
  "name": "react-input-time",
  "version": "3.0.0",
  "description": "A simple, yet customizable, React component for time selection.",
  "author": "Jonathan Wilsson <jonathan.wilsson@gmail.com>",
  "main": "dist/TimeInput.js",
  "types": "dist/TimeInput.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jwilsson/react-input-time.git"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@jwilsson/eslint-config-prettier": "^6.1.0",
    "@jwilsson/eslint-config-typescript": "^6.0.0",
    "@jwilsson/prettier-config": "^1.0.0",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^7.0.0",
    "eslint-config-jwilsson": "^5.0.0",
    "jest": "^27.0.1",
    "prettier": "^2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "simple-git-hooks": "^2.3.1",
    "ts-jest": "^27.0.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "scripts": {
    "build": "tsc --outDir dist",
    "lint": "eslint src/**/*.{ts,tsx} && tsc --noEmit",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test:watch": "jest --coverage --watch",
    "test": "jest --coverage"
  },
  "babel": {},
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write src tests"
  },
  "eslintConfig": {
    "extends": [
      "@jwilsson/eslint-config-typescript",
      "@jwilsson/eslint-config-prettier"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "parserOptions": {
          "project": [
            "./tsconfig.json"
          ]
        }
      }
    ],
    "env": {
      "jest": true
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  },
  "prettier": "@jwilsson/prettier-config",
  "license": "MIT"
}
